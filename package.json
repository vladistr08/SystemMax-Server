{
  "name": "system-max-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "serve": "nodemon src/server.ts",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run serve\"",
    "build": "tsc -p tsconfig.json",
    "check-env": "node -e 'require(\"./dist/config/env\").checkEnv()'",
    "start": "node dist/server.js",
    "prod": "forever start --uid \"${PWD##*/}\" --append dist/server.js",
    "watch-lint": "esw --ext .ts src -w",
    "watch-ts": "tsc -p tsconfig.json -w",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint --ext .ts src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.11",
    "@types/express-serve-static-core": "^4.17.43",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "apollo-server-express": "^3.12.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-watch": "^8.0.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.507.0",
    "@aws-sdk/client-s3": "^3.507.0",
    "@aws-sdk/lib-dynamodb": "^3.507.0",
    "@types/bluebird": "^3.5.42",
    "@types/redis": "^4.0.11",
    "apollo-server": "^3.12.0",
    "bcrypt": "^5.1.1",
    "bluebird": "^3.7.2",
    "express": "^4.18.2",
    "graphql": "^16.7.1",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.27.0",
    "redis": "^4.6.13",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  }
}
